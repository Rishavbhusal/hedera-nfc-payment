  "private": true,
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit --incremental",
    "dev": "node server.mjs",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "ipfs": "NEXT_PUBLIC_IPFS_BUILD=true yarn build && yarn bgipfs upload config init -u https://upload.bgipfs.com && CID=$(yarn bgipfs upload out | grep -o 'CID: [^ ]*' | cut -d' ' -f2) && [ ! -z \"$CID\" ] && echo 'ðŸš€ Upload complete! Your site is now available at: https://community.bgipfs.com/ipfs/'$CID || echo 'âŒ Upload failed'",
    "lint": "next lint",
    "serve": "next start",
    "start": "SKIP_ENV_VALIDATION=true next dev",
    "vercel": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_IGNORE_INCORRECT_LOCKFILE=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:login": "vercel login",
    "vercel:prod": "vercel --prod --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_IGNORE_INCORRECT_LOCKFILE=1 --build-env VERCEL_TELEMETRY_DISABLED=1",
    "vercel:yolo": "vercel --build-env YARN_ENABLE_IMMUTABLE_INSTALLS=false --build-env ENABLE_EXPERIMENTAL_COREPACK=1 --build-env NEXT_IGNORE_INCORRECT_LOCKFILE=1 --build-env NEXT_PUBLIC_IGNORE_BUILD_ERROR=true --build-env VERCEL_TELEMETRY_DISABLED=1",
    "railway:deploy": "railway up",
    "railway:logs": "railway logs",
    "railway:env": "railway variables",
    "railway:status": "railway status"
